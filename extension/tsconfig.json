{
	"compilerOptions": {
      "jsx": "react",
	  "target": "ES2022",
	  "module": "ESNext",
	  "lib": ["ES2022","dom"],
	  "moduleResolution": "Node",
	  "outDir": "dist",
	  "rootDir": "src",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	},
	"include": ["src", "media"],
	"exclude": ["node_modules", "dist"]
  }
  